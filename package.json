{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager .  electron-quick-start --platform=darwin --arch=x64 --ignore=node_modules/electron-*",
    "package": "asar pack electron-quick-start-darwin-x64/electron-quick-start.app/Contents/Resources/app electron-quick-start-darwin-x64/electron-quick-start.app/Contents/Resources/app.asar"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^2.0.0"
  },
  "dependencies": {
    "clear": "^0.1.0",
    "electron-packager": "^12.1.0"
  }
}
